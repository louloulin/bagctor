{
  "name": "bactor-monorepo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "packages/stores/*",
    "packages/agent/examples/*",
    "packages/agent/examples/agents/*",
    "packages/agent/examples/workflows/*"
  ],
  "trustedDependencies": [
    "canvas"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "example:match": "bun run --cwd packages/match-engine example:simple",
    "benchmark:match": "bun run --cwd packages/match-engine benchmark",
    "example:web": "bun run --cwd packages/web example:basic",
    "example:web:middleware": "bun run --cwd packages/web example:middleware",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "devDependencies": {
    "@ai-sdk/cohere": "1.1.14",
    "@ai-sdk/openai": "1.2.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "turbo": "^2.0.0",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.4.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@libp2p/plaintext": "^2.0.13",
    "colorette": "2.0.20",
    "lru-cache": "^11.0.2",
    "qwen-ai-provider": "0.1.0"
  },
  "packageManager": "bun@1.0.28"
}