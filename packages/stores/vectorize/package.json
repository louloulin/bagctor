{
  "name": "@bactor/vectorize",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bun run rimraf dist tsconfig.tsbuildinfo && bun build ./src/index.ts --outdir=./dist --target=node --format=esm --external '*' && bun build ./src/index.ts --outfile=./dist/index.cjs --target=node --format=cjs --external '*'",
    "build:watch": "bun build ./src/index.ts --outdir=./dist --target=node --format=esm --watch --external '*'",
    "test": "vitest run",
    "lint": "eslint .",
    "clean": "bun run rimraf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@bactor/agent": "workspace:^",
    "cloudflare": "^4.0.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.49.2",
    "@types/node": "^22.13.1",
    "tsup": "^8.0.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5",
    "eslint": "^9.20.1"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}