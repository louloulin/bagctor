{
  "name": "@bactor/vectorize",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist tsconfig.tsbuildinfo && tsc -p tsconfig.json && bun build src/index.ts --outfile=dist/index.cjs --target=node --format=cjs",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "vitest run",
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@bactor/agent": "workspace:^",
    "cloudflare": "^4.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.49.2",
    "@types/node": "^22.13.1",
    "tsup": "^8.0.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5",
    "eslint": "^9.20.1"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}